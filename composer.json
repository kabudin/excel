{
  "name": "bud/excel",
  "description": "Excel import and export components adapted to the Hyperf framework",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "hyperf",
    "excel"
  ],
  "autoload": {
    "psr-4": {
      "Bud\\Excel\\": "src/"
    }
  },
  "require": {
    "php": ">=8.1",
    "hyperf/di": "^3.1",
    "hyperf/http-server": "^3.1",
    "hyperf/validation": "^3.1",
    "phpoffice/phpspreadsheet": "^2.0"
  },
  "suggest": {},
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.1-dev"
    },
    "hyperf": {
      "config": "Bud\\Excel\\ConfigProvider"
    }
  }
}
